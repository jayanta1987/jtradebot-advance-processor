{
  "strategy": "SCALPING_ENTRY_LOGIC",
  "version": "2.0",
  "description": "Clean scalping entry logic with only actively used configurations",
  
  "futuresignalsConfig": {
    "enabledTimeframes": [
      "1min",
      "5min"
    ]
  },
  
  "scenarios": [
    {
      "name": "SAFE_ENTRY_SIGNAL",
      "description": "Conservative entry with strong confirmations using scoring config thresholds",
      "requirements": {
        "minQualityScore": 7.5,
        "ema_min_count": 3,
        "futureAndVolume_min_count": 4,
        "candlestick_min_count": 4,
        "momentum_min_count": 4,
        "flatMarketFilter": true
      }
    }
  ],
  
  "callCategories": {
    "ema": [
      "ema5_5min_gt_ema34_5min",
      "ema5_1min_gt_ema34_1min",
      "ema5_15min_gt_ema34_15min"
    ],
    "futureAndVolume": [
      "volume_5min_surge",
      "volume_1min_surge",
      "volume_15min_surge",
      "price_gt_vwap_5min",
      "price_gt_vwap_1min",
      "price_gt_vwap_15min",
      "price_above_resistance"
    ],
    "candlestick": [
      "green_candle_5min",
      "green_candle_1min",
      "long_body_5min",
      "long_body_1min",
      "bullish_engulfing_5min",
      "bullish_engulfing_1min",
      "bullish_morning_star_5min",
      "bullish_morning_star_1min",
      "hammer_5min",
      "hammer_1min"
    ],
    "momentum": [
      "rsi_5min_gt_60",
      "rsi_1min_gt_60",
      "rsi_15min_gt_60",
      "rsi_5min_gt_rsi_ma",
      "rsi_1min_gt_rsi_ma",
      "rsi_15min_gt_rsi_ma"
    ]
  },
  
  "putCategories": {
    "ema": [
      "ema5_5min_lt_ema34_5min",
      "ema5_1min_lt_ema34_1min",
      "ema5_15min_lt_ema34_15min"
    ],
    "futureAndVolume": [
      "volume_5min_surge",
      "volume_1min_surge",
      "volume_15min_surge",
      "price_lt_vwap_5min",
      "price_lt_vwap_1min",
      "price_lt_vwap_15min",
      "price_below_support"
    ],
    "candlestick": [
      "red_candle_5min",
      "red_candle_1min",
      "bearish_engulfing_5min",
      "bearish_engulfing_1min",
      "bearish_evening_star_5min",
      "bearish_evening_star_1min",
      "shooting_star_5min",
      "shooting_star_1min"
    ],
    "momentum": [
      "rsi_5min_lt_40",
      "rsi_1min_lt_40",
      "rsi_15min_lt_40",
      "rsi_5min_lt_rsi_ma",
      "rsi_1min_lt_rsi_ma",
      "rsi_15min_lt_rsi_ma"
    ]
  },
  
  "flatMarketFiltering": {
    "enabled": true,
    "description": "Filter to prevent entries in flat or non-directional markets",
    "requirements": {
      "minDirectionalStrength": 0.35,
      "maxConsecutiveDoji": 2,
      "maxConsecutiveSpinningTop": 2,
      "maxConsecutiveSmallCandles": 5
    },
    "thresholds": {
      "volatilityScore": {
        "lowThreshold": 0.35,
        "veryLowThreshold": 0.15
      },
      "directionalStrength": {
        "veryLowThreshold": 0.25
      },
      "comprehensiveChecks": {
        "emaAlignmentScore": 0.35,
        "volumeConsistencyScore": 0.35,
        "priceActionScore": 0.45,
        "overallScore": 0.35
      },
      "flatMarketAdjustments": {
        "categoryIncrement": 1,
        "minQualityScore": 7.0
      },
      "volumeConsistency": {
        "minVolumeMultiplier": 1.2,
        "requiredVolumeSignals": 1
      }
    },
    "atrSettings": {
      "period": 14,
      "minATR": 20.0
    },
    "lookbackSettings": {
      "candleAnalysis": 10,
      "volatilityAnalysis": 10
    }
  },
  
  "tradingConfiguration": {
    "riskManagement": {
      "rsiThresholds": {
        "callRsiThreshold": 60.0,
        "putRsiThreshold": 40.0,
        "rsiMaPeriod": 14,
        "enableRsiMaComparison": true
      },
      "stopLossPercentage": 0.3,
      "targetPercentage": 0.8,
      "stopLossPoints": 10.0,
      "targetPoints": 20.0
    },
    "tradeSettings": {
      "maxTradeHoldingTimeInSec": 300
    }
  }
}

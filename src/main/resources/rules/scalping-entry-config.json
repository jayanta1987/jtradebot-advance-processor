{
  "strategy": "SCALPING_ENTRY_LOGIC",
  "version": "2.1",
  "description": "Clean scalping entry logic with individual indicator weightage-based scoring system",
  
  "futuresignalsConfig": {
    "enabledTimeframes": [
      "1min",
      "5min"
    ]
  },
  
  "scenarios": [
    {
      "name": "SAFE_ENTRY_SIGNAL",
      "description": "Conservative entry with strong confirmations using category-based scoring system",
      "requirements": {
        "minQualityScore": 8.5,
        "ema_min_score": 2.5,
        "futureAndVolume_min_score": 5,
        "candlestick_min_score": 3.5,
        "momentum_min_score": 2.5,
        "flatMarketFilter": true
      }
    }
  ],
  
  "categoryScoring": {
    "callCategories": {
      "ema": {
        "indicators": {
          "ema5_5min_gt_ema34_5min": 2,
          "ema5_1min_gt_ema34_1min": 0.5,
          "ema5_15min_gt_ema34_15min": 1
        }
      },
      "futureAndVolume": {
        "indicators": {
          "volume_5min_surge": 2,
          "volume_1min_surge": 0.5,
          "volume_15min_surge": 1,

          "price_gt_vwap_5min": 2,
          "price_gt_vwap_1min": 0.5,
          "price_gt_vwap_15min": 1,

          "oi_bullish_signal_5min": 2,
          "oi_bullish_signal_1min": 0.5,
          "oi_bullish_signal_15min": 1,

          "price_above_resistance": 2
        }
      },
      "candlestick": {
        "indicators": {
          "green_candle_5min": 1,
          "green_candle_1min": 0.5,
          "long_body_5min": 1,
          "long_body_1min": 0.5,
          "bullish_engulfing_5min": 2,
          "bullish_engulfing_1min": 1,
          "bullish_morning_star_5min": 2,
          "bullish_morning_star_1min": 1,
          "hammer_5min": 1,
          "hammer_1min": 1,
          "marubozu_1min": 1,
          "marubozu_5min": 2,
          "inside_bar_breakout_5min": 2,
          "wick_rejection_filter_1min": 2
        }
      },
      "momentum": {
        "indicators": {
          "rsi_5min_gt_60": 1,
          "rsi_1min_gt_60": 0.5,
          "rsi_5min_gt_rsi_ma": 1,
          "rsi_1min_gt_rsi_ma": 0.5,
          "rsi_bullish_divergence_5min": 2,
          "rsi_bullish_divergence_15min": 2
        }
      }
    },
    
    "putCategories": {
      "ema": {
        "indicators": {
          "ema5_5min_lt_ema34_5min": 2,
          "ema5_1min_lt_ema34_1min": 0.5,
          "ema5_15min_lt_ema34_15min": 1
        }
      },
      "futureAndVolume": {
        "indicators": {
          "volume_5min_surge": 2,
          "volume_1min_surge": 0.5,
          "volume_15min_surge": 1,

          "price_lt_vwap_5min": 2,
          "price_lt_vwap_1min": 0.5,
          "price_lt_vwap_15min": 1,

          "price_below_support": 2,

          "oi_bearish_signal_5min": 2,
          "oi_bearish_signal_1min": 0.5,
          "oi_bearish_signal_15min": 1
        }
      },
      "candlestick": {
        "indicators": {
          "red_candle_5min": 1,
          "red_candle_1min": 0.5,
          "bearish_engulfing_5min": 2,
          "bearish_engulfing_1min": 1,
          "bearish_evening_star_5min": 2,
          "bearish_evening_star_1min": 1,
          "shooting_star_5min": 1,
          "shooting_star_1min": 1,
          "marubozu_1min": 1,
          "marubozu_5min": 2,
          "inside_bar_breakdown_5min": 2,
          "wick_rejection_filter_1min": 2
        }
      },
      "momentum": {
        "indicators": {
          "rsi_5min_lt_40": 1,
          "rsi_1min_lt_40": 0.5,
          "rsi_5min_lt_rsi_ma": 1,
          "rsi_1min_lt_rsi_ma": 0.5,
          "rsi_bearish_divergence_5min": 2,
          "rsi_bearish_divergence_15min": 2
        }
      }
    }
  },
  
  "noTradeZones": {
    "enabled": true,
    "description": "Weightage-based market condition filtering using NTP (No Trading Point) system",
    "maxAllowedNTP": 1,
    "filters": {
      "candleHeight": {
        "enabled": true,
        "ntp": 1,
        "name": "Minimum Candle Height",
        "description": "Filter out entries with candle height less than 8.0",
        "threshold": 4.0,
        "priority": 1
      },
      "volumeSurge": {
        "enabled": true,
        "ntp": 1,
        "name": "Volume Surge Multiplier",
        "description": "Filter out entries with volume surge less than configured threshold",
        "threshold": 8.0,
        "priority": 2
      },
      "bodyRatio": {
        "enabled": true,
        "ntp": 1,
        "name": "Candle Body Ratio",
        "description": "Filter out entries with body ratio less than 0.50",
        "threshold": 0.50,
        "priority": 3
      },
      "ema200Distance": {
        "enabled": true,
        "ntp": 2,
        "name": "EMA 200 Distance",
        "description": "Filter out entries with EMA 200 distance more than 0.6% or less than 0.1% of EMA 200",
        "maxThreshold": 0.006,
        "minThreshold": 0.002,
        "priority": 4
      },
      "priceBetweenEma34AndEma200": {
        "enabled": true,
        "ntp": 0.5,
        "name": "Price Between EMA34 and EMA200",
        "description": "Filter out entries when price is between EMA34 and EMA200",
        "priority": 5
      },
      "overboughtOversold": {
        "enabled": true,
        "ntp": 2,
        "name": "Overbought/Oversold RSI",
        "description": "Filter out entries when RSI is overbought (>80) or oversold (<20)",
        "overboughtThreshold": 80.0,
        "oversoldThreshold": 20.0,
        "priority": 6
      },
      "atr5Min": {
        "enabled": true,
        "ntp": 1,
        "name": "ATR 5-Minute Filter",
        "description": "Filter out entries when 5-minute ATR is less than 15.0 (low volatility)",
        "threshold": 15.0,
        "priority": 7
      },
      "consecutiveSameColorCandles": {
        "enabled": true,
        "ntp": 2,
        "name": "Consecutive Same Color Candles",
        "description": "BLOCK entries when there are too many consecutive same color candles (negative signal - indicates overextension)",
        "maxConsecutiveCount": 5,
        "analysisWindow": 7,
        "priority": 8,
        "timeframe": "FIVE_MIN"
      },
      "nearToSupportResistance": {
        "enabled": true,
        "ntp": 1,
        "name": "Near Support/Resistance Filter",
        "description": "Filter out entries when price is very near support/resistance levels or round figure index prices",
        "priority": 9,
        "supportResistanceBuffer": 5,
        "roundFigureBuffer": 5
      },
      "tradingHours": {
        "enabled": true,
        "ntp": 2,
        "name": "Trading Hours Filter",
        "description": "Filter out entries outside of trading hours (9:15 AM to 3:30 PM IST)",
        "priority": 10,
        "startHour": 9,
        "startMinute": 15,
        "endHour": 15,
        "endMinute": 15
      }
    }
  },
  

  
  "tradingConfiguration": {
    "quantitySettings": {
      "minLotSize": 225,
      "maxQuantityPerTrade": 550
    },
    "investmentLimits": {
      "maxInvestmentPercentage": 5.0
    },
    "riskManagement": {
      "maxRiskPerDayPercentage": 2.0,
      "maxProfitPerDayPercentage": 5.0,
      "milestonePoints": 7.0,
      "rsiThresholds": {
        "callRsiThreshold": 60.0,
        "putRsiThreshold": 40.0,
        "rsiMaPeriod": 14,
        "enableRsiMaComparison": true
      },
      "volumeSurgeMultiplier": 1.5,
      "signalStrength": 0.7,
      "stopLossPercentage": 0.3,
      "targetPercentage": 0.8,
      "stopLossPoints": 10.0,
      "targetPoints": 20.0,
      "dynamicRiskManagement": {
        "enabled": false,
        "stopLossPercentage": 0.25,
        "targetMultiplier": 2.5,
        "minStopLoss": 5.0,
        "maxStopLoss": 10.0,
        "minTarget": 10.0,
        "maxTarget": 30.0,
        "fallbackValues": {
          "defaultStopLossWhenDisabled": 10.0,
          "defaultTargetWhenDisabled": 20.0,
          "defaultVolatilityScore": 0.5
        },
        "technicalParameters": {
          "minRequiredBars": 20,
          "volatilityLookbackPeriod": 14,
          "candleTimeframe": "FIVE_MIN"
        }
      }
    },
    "tradeSettings": {
      "maxTradeHoldingTimeInSec": 150
    },
    "exitSignalConfiguration": {
      "enabled": true,
      "description": "Exit signal configuration to match relaxed entry criteria",
      "exitThresholds": {
        "callExitThreshold": 0.5,
        "putExitThreshold": 0.5,
        "description": "Threshold for exit signals (0.5 = 50% of signals must be in opposite direction)"
      },
      "rsiDivergenceExit": {
        "enabled": true,
        "description": "Exit on RSI divergence signals"
      },
      "marketConditionExit": {
        "enabled": true,
        "description": "Exit on market condition reversal"
      }
    }
  },
  "marketEndScheduler": {
    "enabled": true,
    "description": "Close all active trades when market hours are crossed (runs every 5 minutes to check)"
  }
}
